{
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "parts:start": "npx @11ty/eleventy --serve",
    "parts:build": "npx @11ty/eleventy",
    "parts:clean": "rimraf dist",
    "build:prod": "cross-env DEPLOY_ENV=prod run-s parts:clean parts:build",
    "build:dev": "cross-env DEPLOY_ENV=dev run-s parts:clean parts:build",
    "start:prod": "cross-env DEPLOY_ENV=prod run-s parts:clean parts:start",
    "start:dev": "cross-env DEPLOY_ENV=dev run-s parts:clean parts:start"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@wooorm/starry-night": "^2.1.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.2",
    "esbuild-sass-plugin": "^2.15.0",
    "evaluate-value": "^2.0.0",
    "hast-util-to-html": "^9.0.0",
    "http-equiv-refresh": "^2.0.1",
    "js-yaml": "^4.1.0",
    "list-to-array": "^1.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-for-inline": "^0.1.1",
    "npm-run-all": "^4.1.5",
    "parse-srcset": "^1.0.2",
    "patch-package": "^8.0.0",
    "posthtml": "^0.16.6",
    "rimraf": "^5.0.1",
    "slugify": "^1.6.6"
  },
  "dependencies": {
    "dayjs": "^1.11.9",
    "duration": "^0.2.2",
    "feather-icons": "^4.29.1",
    "normalize.css": "^8.0.1",
    "ordinal": "^1.0.3"
  }
}
